{
    "name": "node-vue-Etranslate",
    "version": "0.1.0",
    "engines": {
        "node": "12.16.1"
    },
    "type": "module",
    "private": true,
    "scripts": {
        "lint": "vue-cli-service lint",
        "cli-dev": "vue-cli-service serve client/src",
        "cli-debug": "cross-env NODE_ENV=production vue-cli-service serve client/src",
        "cli-prod": "vue-cli-service build client/src",
        "cli-test": "vue-cli-service test:unit --watch",
        "server-dev": "nodemon --inspect --experimental-modules --es-module-specifier-resolution=node --watch server server/index.js",
        "server-prod": "cross-env NODE_ENV=production node --experimental-modules --es-module-specifier-resolution=node server/index.js"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.13.0",
        "array-from-polyfill": "^1.0.1",
        "axios": "^0.20.0",
        "babel-polyfill": "^6.26.0",
        "body-parser": "^1.19.0",
        "config": "^3.3.1",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "cross-env": "^5.2.1",
        "eslint-plugin-prettier": "^3.1.3",
        "expo-server-sdk": "^3.6.0",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "googleapis": "^39.1.0",
        "helmet": "^3.22.0",
        "ibm-watson": "^5.7.0",
        "install": "^0.12.2",
        "isomorphic-fetch": "^2.2.1",
        "js-cache": "^1.0.3",
        "jsonwebtoken": "^8.5.1",
        "material-icons": "^0.3.1",
        "multer": "^1.4.2",
        "node-schedule": "^1.3.2",
        "nodemailer": "^4.7.0",
        "npm": "^6.14.5",
        "passport": "^0.4.1",
        "passport-google-oauth": "^2.0.0",
        "passport-jwt": "^4.0.0",
        "passport-yandex": "0.0.3",
        "pg": "^7.18.2",
        "pg-hstore": "^2.3.3",
        "postgresql": "0.0.1",
        "sequelize": "^4.44.4",
        "simple-keyboard": "^2.29.41",
        "simple-keyboard-layouts": "^1.13.9",
        "tesseract.js": "^2.1.4",
        "url": "^0.11.0",
        "vue": "^2.6.11",
        "vue-fragment": "^1.5.1",
        "vue-router": "^3.3.2",
        "vue-typed-js": "^0.1.2",
        "vuetify": "^1.5.24",
        "vuex": "^3.4.0",
        "vuex-router-sync": "^5.0.0",
        "winston": "^3.2.1",
        "yandex-dictionary": "0.0.0"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.10.2",
        "@vue/cli-plugin-babel": "^3.12.1",
        "@vue/cli-plugin-eslint": "^3.12.1",
        "@vue/cli-plugin-unit-jest": "^3.12.1",
        "@vue/cli-service": "^3.12.1",
        "@vue/eslint-config-airbnb": "^4.0.1",
        "@vue/test-utils": "^1.0.3",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^23.6.0",
        "eslint": "^5.16.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-vue": "^5.2.3",
        "jest": "^18.1.0",
        "jest-fetch-mock": "^2.1.2",
        "jest-localstorage-mock": "^2.4.2",
        "node-sass": "^7.0.0",
        "nodemon": "^1.18.5",
        "sass-loader": "^7.3.1",
        "stylus": "^0.54.7",
        "stylus-loader": "^3.0.1",
        "vue-cli-plugin-vuetify": "^0.4.5",
        "vue-template-compiler": "^2.6.11",
        "vuetify-loader": "^1.4.4"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true,
            "jest": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended",
            "@vue/airbnb",
            "prettier"
        ],
        "plugins": [
            "prettier"
        ],
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "rules": {
            "no-param-reassign": "off",
            "class-methods-use-this": "off"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
